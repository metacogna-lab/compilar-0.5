{
  "name": "compilar-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist",
    "start": "bun run dist/index.js",
    "setup-env": "bun run setup-env.js",
    "verify-setup": "bun run verify-setup.js",
    "test-health": "bun run test-health.js",
    "test": "bun test tests/integration",
    "test:watch": "bun test --watch tests/integration",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "@hono/zod-validator": "^0.1.0",
    "@supabase/supabase-js": "^2.38.0",
    "zod": "^3.22.0",
    "dotenv": "^16.3.0",
    "@anthropic-ai/sdk": "^0.30.0",
    "openai": "^4.73.0",
    "langsmith": "^0.2.0",
    "@langchain/core": "^0.3.0"
  },
  "devDependencies": {
    "bun-types": "^1.0.0",
    "@types/node": "^20.0.0",
    "supabase": "^1.127.0"
  }
}